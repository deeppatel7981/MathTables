import java.util.*;
public class Main {
  
  public static void main(String[] args) {
  // Write your code here
  int a, b, temp;
  a = 15;
  b = 27;
  System.out.println("Before Swapping a =" + a + " and b =" + b );
  temp = a;
  a = b;
  b = temp;
  System.out.println("After Swapping a =" + a + " and b =" + b );
  
}
}
