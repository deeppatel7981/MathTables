import java.util.*;
public class Main 
{
   public static void main(String[] args)
  {
  // Write your code here
  Scanner in = new Scanner(System.in);
  
  System.out.println("Enter an Input: ");
  int n = in.nextInt();
  
  for(int i = 0; i<=10; i++)
  {
    System.out.println(n+"*"+i+"="+(n*i));
  }
  
}
}
